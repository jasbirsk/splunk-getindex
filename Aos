index=aos_core a=servicedebugger logger="com.xyz.store.platform.servicedebugger.services.HealthCheckService" "SD_VIP_HEALTH_CHECK_AUDIT"
| rex "vip: {}=\"(?<vip>.*)\"; status"  | rex "status: {}=\"(?<status>.*)\"; " 
| search service=ARDeliveryService vip="https://ardeliveryservice.pd04aws.aosp.xyz.com/graphql"
| where _time >= relative_time(now(), "-1h") AND status="DOWN"
| bin _time span=15m
| stats count as DownCount by _time, service, vip, status
| where DownCount > 0
| table _time, service, vip, status, DownCount
